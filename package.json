    "aws-amplify": "^4.3.8",
    "clsx": "^1.1.1",
    "papaparse": "^5.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-dropzone": "^12.0.1",
    "react-hook-form": "^7.24.1",
    "react-hot-toast": "^2.1.1",
    "react-query": "^3.34.0",
    "react-router-dom": "^6.0.2",
    "react-table": "^7.7.0",
    "react-table-plugins": "^1.3.2",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.2"
